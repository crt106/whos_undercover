{
  "name": "whos-undercover",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "npm run dev --prefix client -- --host",
    "build": "npm run build --prefix client",
    "start": "npm run build && NODE_ENV=production node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "concurrently": "^8.2.2"
  }
}
